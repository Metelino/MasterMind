<StackPane fx:controller="org.openjfx.GraController" xmlns:fx="http://javafx.com/fxml" fx:id="GameStack">
    <VBox fx:id = "GameScreen" alignment="CENTER" spacing="15">
        <GridPane alignment="CENTER" hgap="35" vgap="15" fx:id = "GameGrid" maxWidth="-Infinity" maxHeight="-Infinity"/>
        <Button prefWidth="200"
                style = "-fx-font-size : 2em;" text = "Check Colors" onMouseClicked="#checkButton"/>
    </VBox>

    <TilePane maxHeight="-Infinity" maxWidth="-Infinity"
              layoutX="0" layoutY="0"
              alignment="CENTER"
              hgap="3" vgap="3" visible="false" mouseTransparent="true" fx:id="Palette"/>

</StackPane>







